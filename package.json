{
  "name": "@dojo/webpack-contrib",
  "version": "3.0.3-pre",
  "description": "Webpack loaders and plugins used by the Dojo build process",
  "engines": {
    "npm": ">=3.0.0"
  },
  "private": true,
  "homepage": "http://dojo.io",
  "bugs": {
    "url": "https://github.com/dojo/webpack-contrib/issues"
  },
  "license": "(BSD-3-Clause OR Apache-2.0)",
  "main": "main.js",
  "files": [
    "dist",
    "src",
    "typings.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dojo/webpack-contrib.git"
  },
  "scripts": {
    "webpack-bundle-analyzer": "npm run webpack-bundle-analyzer:babel && npm run webpack-bundle-analyzer:webpack",
    "webpack-bundle-analyzer:babel": "npm-run-all -p webpack-bundle-analyzer:babel:*",
    "webpack-bundle-analyzer:babel:dist": "babel src/webpack-bundle-analyzer/src --out-dir dist/release/webpack-bundle-analyzer",
    "webpack-bundle-analyzer:babel:dev": "babel src/webpack-bundle-analyzer/src --out-dir dist/dev/src/webpack-bundle-analyzer",
    "webpack-bundle-analyzer:webpack": "npm-run-all -p webpack-bundle-analyzer:webpack:*",
    "webpack-bundle-analyzer:webpack:dist": "cd src/webpack-bundle-analyzer && webpack ",
    "webpack-bundle-analyzer:webpack:dev": "cd src/webpack-bundle-analyzer && webpack --env.prod=true",
    "build:static:dev": "cpx \"{src,tests}/**/*.{d.ts,html,md,json,js,css,ejs}\" dist/dev",
    "build:static:release": "cpx \"src/**/*.{d.ts,json,ejs}\" dist/release",
    "build:cjs": "tsc",
    "build": "npm-run-all -p build:** -s webpack-bundle-analyzer -s dojo-package",
    "clean": "rimraf dist coverage",
    "dojo-package": "dojo-package",
    "dojo-release": "dojo-release",
    "intern": "intern",
    "lint:tslint": "tslint -p .",
    "lint:prettier": "prettier -l \"{src,tests}/**/*.{ts,tsx}\"",
    "lint": "run-p lint:*",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"{src,tests}/**/*.{ts,tsx}\"",
    "release": "run-s lint clean build \"dojo-release -- {@}\" --",
    "test": "run-s lint build intern",
    "uploadCoverage": "codecov --file=coverage/coverage.json",
    "watch:ts": "dojo-tsc-watcher -p tsconfig.json -- dojo-package",
    "watch": "run-p watch:ts \"build:static:** -- --watch\""
  },
  "dependencies": {
    "@dojo/framework": "~3.0.0",
    "acorn-dynamic-import": "3.0.0",
    "copy-webpack-plugin": "4.0.1",
    "filter-css": "0.1.2",
    "html-webpack-include-assets-plugin": "1.0.2",
    "istanbul-lib-instrument": "1.10.1",
    "jsdom": "11.6.2",
    "loader-utils": "1.1.0",
    "recast": "0.12.7",
    "source-map": "0.6.1",
    "ts-loader": "3.1.1",
    "typed-css-modules": "0.3.1",
    "typescript": "~2.6.1",
    "workbox-webpack-plugin": "3.2.0",
    "acorn": "^5.3.0",
    "bfj-node4": "^5.2.0",
    "chalk": "^2.3.0",
    "commander": "^2.13.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "filesize": "^3.5.11",
    "gzip-size": "^4.1.0",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "opener": "^1.4.3",
    "ws": "^4.0.0"
  },
  "devDependencies": {
    "@dojo/scripts": "~3.0.1",
    "@types/estree": "0.0.37",
    "@types/loader-utils": "^1.1.3",
    "@types/mockery": "1.4.29",
    "@types/node": "9.6.5",
    "@types/sinon": "~4.3.3",
    "@types/tapable": "0.2.5",
    "@types/webpack": "3.8.1",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-env": "1.6.1",
    "chai": "4.1.2",
    "chai-subset": "1.6.0",
    "classnames": "2.2.5",
    "codecov": "~3.0.4",
    "cpx": "~1.5.0",
    "css-loader": "0.28.9",
    "d3": "3.5.17",
    "del": "3.0.0",
    "eslint": "4.16.0",
    "eslint-config-th0r": "1.0.0",
    "eslint-config-th0r-react": "1.0.0",
    "eslint-plugin-react": "7.6.1",
    "exports-loader": "0.6.4",
    "husky": "0.14.3",
    "lint-staged": "6.0.0",
    "mockery": "2.1.0",
    "nightmare": "2.10.0",
    "normalize-newline": "3.0.0",
    "npm-run-all": "~4.1.3",
    "preact": "8.2.7",
    "rimraf": "~2.6.2",
    "sinon": "~4.5.0",
    "stream-combiner2": "1.1.1",
    "style-loader": "0.20.1",
    "webpack": "3.8.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 4,
    "useTabs": true,
    "parser": "typescript",
    "printWidth": 120,
    "arrowParens": "always"
  }
}
